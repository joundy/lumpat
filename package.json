{
  "name": "lumpat",
  "displayName": "lumpat",
  "publisher": "Joundy",
  "repository": "https://github.com/joundy/lumpat",
  "description": "An easy motion extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lumpat.jump",
        "title": "Highlight Jump Texts"
      }
    ],
    "keybindings": [
      {
        "command": "lumpat.a",
        "key": "a",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.b",
        "key": "b",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.c",
        "key": "c",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.d",
        "key": "d",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.e",
        "key": "e",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.f",
        "key": "f",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.g",
        "key": "g",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.h",
        "key": "h",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.i",
        "key": "i",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.j",
        "key": "j",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.k",
        "key": "k",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.l",
        "key": "l",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.m",
        "key": "m",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.n",
        "key": "n",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.o",
        "key": "o",
        "when": "lumpat.jump-mode && editorTextFocus"
