{
  "name": "Lumpat",
  "displayName": "lumpat",
  "publisher": "Joundy",
  "repository": {
    "type": "git",
    "url": "https://github.com/joundy/lumpat"
  },
  "description": "A VS Code extension that enhances text navigation with quick jump-to-highlight functionality (vim easy motion like).",
  "version": "0.1.6",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "jumpy",
    "jumpy2",
    "jump",
    "mouseless",
    "cursor",
    "vim",
    "nvim",
    "neovim",
    "hop",
    "sneak",
    "easy motion"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "icon": "assets/lumpat_logo.png",
  "contributes": {
    "commands": [
      {
        "command": "lumpat.jump",
        "title": "Highlight Jump Texts"
      },
      {
        "command": "lumpat.close",
        "title": "Close Jump Mode"
      }
    ],
    "keybindings": [
      {
        "command": "lumpat.jump",
        "key": "shift+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "lumpat.close",
        "key": "escape",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.a",
        "key": "a",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.b",
        "key": "b",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.c",
        "key": "c",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.d",
        "key": "d",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.e",
        "key": "e",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.f",
        "key": "f",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.g",
        "key": "g",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.h",
        "key": "h",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.i",
        "key": "i",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.j",
        "key": "j",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.k",
        "key": "k",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.l",
        "key": "l",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.m",
        "key": "m",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.n",
        "key": "n",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.o",
        "key": "o",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.p",
        "key": "p",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.q",
        "key": "q",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.r",
        "key": "r",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.s",
        "key": "s",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.t",
        "key": "t",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.u",
        "key": "u",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.v",
