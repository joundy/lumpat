      },
      {
        "command": "lumpat.s",
        "key": "s",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.t",
        "key": "t",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.u",
        "key": "u",
        "when": "lumpat.jump-mode && editorTextFocus"
      },
      {
        "command": "lumpat.v",
